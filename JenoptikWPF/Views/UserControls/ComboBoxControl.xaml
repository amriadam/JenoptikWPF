<UserControl x:Class="JenoptikWPF.Views.UserControls.ComboBoxControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:JenoptikWPF.Views.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <Label x:Name="mHeaderLabel" Content="This is my label"  />
        <ComboBox x:Name="mComboBox"  Foreground="#006EA9" Height="35" Padding="10" ></ComboBox>
    </StackPanel>
</UserControl>
